<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EquacaoFundamental">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="71dp"
        android:layout_y="21dp"
        android:text="Equação Fundamental"
        android:textAlignment="center"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="228dp"
        tools:layout_editor_absoluteY="83dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="5dp"
        android:layout_y="116dp"
        android:text="Insira o comprimento de onda:"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="110dp" />

    <EditText
        android:id="@+id/txtComp"
        android:layout_width="154dp"
        android:layout_height="wrap_content"
        android:layout_x="251dp"
        android:layout_y="100dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="156dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="3dp"
        android:layout_y="197dp"
        android:text="Insira a frequência da onda:"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="188dp" />

    <EditText
        android:id="@+id/txtFrequencia"
        android:layout_width="181dp"
        android:layout_height="wrap_content"
        android:layout_x="227dp"
        android:layout_y="180dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="173dp"
        tools:layout_editor_absoluteY="209dp" />

    <Button
        android:id="@+id/buttonCalc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="157dp"
        android:layout_y="276dp"
        android:text="Calcular"
        android:onClick="buttonCalc"
        tools:layout_editor_absoluteX="150dp"
        tools:layout_editor_absoluteY="264dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="3dp"
        android:layout_y="361dp"
        android:text="A velocidade da onda corresponde a:"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="365dp" />

    <TextView
        android:id="@+id/txtResult"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        android:layout_x="1dp"
        android:layout_y="392dp"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="299dp"
        tools:layout_editor_absoluteY="365dp" />
</AbsoluteLayout>